<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="275.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ToolBar fx:id="toolbar" maxWidth="1.7976931348623157E308">
                    <items>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                              <Label text="Filter: " />
                              <TextField fx:id="tf_search" />
                     </children>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </HBox>
                    </items>
                </ToolBar>
                <TabPane fx:id="tabpane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS">
                    <tabs>
                        <Tab closable="false" text="GoogleMaterial">
                            <content>
                                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </ScrollPane>
                            </content>
                        </Tab>
                        <Tab closable="false" text="CommunityMaterial">
                            <content>
                                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <content>
                                        <VBox fx:id="vbox_container1" spacing="5" />
                                    </content>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </ScrollPane>
                            </content>
                        </Tab>
                        <Tab closable="false" text="FontAwesome">
                            <content>
                                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <content>
                                        <VBox fx:id="vbox_container2" spacing="5" />
                                    </content>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </ScrollPane>
                            </content>
                        </Tab>
                        <Tab closable="false" text="Octicons">
                            <content>
                                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <content>
                                        <VBox fx:id="vbox_container3" spacing="5" />
                                    </content>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </ScrollPane>
                            </content>
                        </Tab>
                        <Tab closable="false" text="Meteocons">
                            <content>
                                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <content>
                                        <VBox fx:id="vbox_container4" spacing="5" />
                                    </content>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </ScrollPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
                <HBox alignment="CENTER" spacing="5.0">
                    <children>
                        <Slider fx:id="slider_size" max="72.0" min="6.0" minorTickCount="1" snapToTicks="true" />
                        <Text fx:id="t_fontsize" strokeType="OUTSIDE" strokeWidth="0.0" text="font size: " />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
